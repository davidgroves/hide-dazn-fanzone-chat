{
  "manifest_version": 3,
  "name": "Hide DAZN Fanzone Chat",
  "version": "0.0.1",
  "description": "Hides the DAZN Fanzone Chat Sidepanel.",
  "icons": {
    "48": "icons/hidechat-48.png",
    "96": "icons/hidechat-96.png",
    "460": "icons/hidechat-460.png"
  },
  "content_scripts": [
    {
      "js": [
        "hide-fanzone.js"
      ],
      "matches": [
        "https://*.dazn.com/*"
      ]
    }
  ]
}
